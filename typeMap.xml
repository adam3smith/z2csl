      <var name="medium" type="standard" description="medium description (e.g. &quot;CD&quot;, &quot;DVD&quot;, etc.)" />
      <var name="note" type="standard" description="(short) inline note giving additional item details (e.g. a concise summary or commentary)" />
      <var name="original-publisher" type="standard" description="original publisher, for items that have been republished by a different publisher" />
